{
  "name": "hola-mundo",
  "version": "1.0.0",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:back": "nodemon --watch be-src --exec npx tsx ./be-src/server/index.ts",
    "dev:front": "parcel serve ./fe-src/index.html",
    "build:front": "parcel build ./src/index.html --dist-dir build --public-url /single-web-app",
    "build:back": "npx tsc",
    "run": "npx tsx ./src/server/index.ts",
    "build": "npm run build:front && npm run build:back",
    "start": "node ./build/server/index.js"
  },
  "keywords": [
    "algolia",
    "pet finder",
    "postgress"
  ],
  "author": "Caio Puyol",
  "license": "ISC",
  "description": "Primer proyecto usando algolia.",
  "dependencies": {
    "algoliasearch": "^4.25.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "events": "^3.3.0",
    "nodemon": "^3.1.10",
    "os-browserify": "^0.3.0",
    "parcel": "^2.16.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "string_decoder": "^1.3.0",
    "tsx": "^4.20.6",
    "vm-browserify": "^1.1.2"
  }
}
